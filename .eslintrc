{
    "extends": [
        "eslint:recommended", "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 1,
        "no-underscore-dangle": 1,
        "quotes": [2, "single"],
        "no-constant-condition": 2,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-irregular-whitespace": 2,
        "no-unreachable": 2,
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-const-assign": 2,
        "semi": [2, "always"],

        "react/prop-types": 0
    }
}
